  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kyle-rader/puzzlehunt/issues"
  },
  "homepage": "https://github.com/kyle-rader/puzzlehunt#readme",
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "bcrypt": "^0.8.7",
    "brace": "^0.8.0",
    "clipboard": "^1.5.16",
    "highlight.js": "^9.8.0",
    "markdown-it": "^8.1.0",
    "meteor-node-stubs": "^0.2.3",
    "moment": "^2.14.1",
    "qrcode.react": "^0.6.1",
    "react": "^15.1.0",
    "react-ace": "^3.5.0",
    "react-addons-pure-render-mixin": "^15.1.0",
    "react-dom": "^15.3.0",
    "react-mounter": "^1.2.0",
    "react-router": "^3.0.0",
