{
  "name": "wwupuzzlehunt",
  "version": "1.0.0",
  "description": "A Puzzle Hunt Game System",
  "main": "main.js",
  "scripts": {
    "start": "npm run meteor:dev",
    "test": "meteor test --driver-package practicalmeteor:mocha --port 3100",
    "meteor:dev": "meteor --settings settings-dev.json || true",
    "mup:set:prod": "cp mup-prod.js mup.js && cp settings-prod.json settings.json",
    "mup:deploy": "mup deploy --config=mup-prod.js --settings=settings-prod.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyle-rader/puzzlehunt.git"
  },
